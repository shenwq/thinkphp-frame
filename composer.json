{
  "name": "ffhome/thinkphp-frame",
  "description": "ThinkPHP frame.",
  "keywords": [
    "ThinkPHP"
  ],
  "homepage": "http://ffhome.top/",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "john.shen",
      "email": "john.shen@foxmail.com"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "topthink/framework": "^6.0.0",
    "topthink/think-orm": "^2.0",
    "jianyan74/php-excel": "^1.0",
    "ffhome/jwt-auth": "^1.0",
    "ffhome/thinkphp-upload": "^1.1.5",
    "ffhome/php-common": "^1.3.19"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "ffhome\\frame\\": "src/"
    },
    "files": [
      "src/helper.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "ffhome\\tests\\frame\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  }
}
